<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>

<AnchorPane prefWidth="300" prefHeight="200" xmlns:fx="http://javafx.com/fxml">
    <children>
        <TextField fx:id="usernameField" layoutX="80" layoutY="40" promptText="Username" />
        <PasswordField fx:id="passwordField" layoutX="80" layoutY="80" promptText="Password" />
        <Button text="Login" fx:id="loginButton" layoutX="120" layoutY="130" />
    </children>
</AnchorPane>
